#!/usr/bin/env ruby

$: << File.expand_path('../../lib', __FILE__)

require 'bundler'; Bundler.setup(:default)
require 'adtech'
require 'date'
require 'open-uri'
require 'csv'

ADTech::Client.region = ADTech::EU_SERVER
report = ADTech::API::Report.new
url = report.get_report_url(1293, Date.today - 1, Date.today - 1, [1185844, 1185854])
puts CSV.new(open(url), headers: :first_row).inspect
